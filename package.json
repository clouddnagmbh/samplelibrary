{
	"name": "samplelibrary_library",
	"version": "1.0.0",
	"description": "samplelibrary SAPUI5 Control Library",
	"author": "Maximilian Olzinger",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/SAP-samples/ui5-typescript-control-library.git"
	},
	"types": "dist/index.d.ts",
	"scripts": {
		"doc": "typedoc src/Comments.ts",
		"build:ts-interfaces": "npx @ui5/ts-interface-generator",
		"build:jsdoc": "ui5 build jsdoc --config ui5-jsdoc.yaml --exclude-task buildThemes",
		"start": "npm run build:ts-interfaces --watch && npm run start:server",
		"start:server": "ui5 serve --port 8080 -o test-resources/at/clouddna/samplelibrary/Example.html",
		"start:dist": "ui5 serve  --port 8080 -o test-resources/at/clouddna/samplelibrary/Example.html --config ui5-dist.yaml",
		"testsuite": "ui5 serve --open test-resources/at/clouddna/samplelibrary/qunit/testsuite.qunit.html",
		"ts-typecheck": "tsc --noEmit",
		"format": "prettier --write .",
		"lint": "eslint src test",
		"test": "npm run lint && npm run karma-ci-cov",
		"deploy-config": "fiori add deploy-config",
		"deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
		"build": "npm run clean && ui5 build --exclude-task buildThemes --include-task=generateManifestBundle generateCachebusterInfo && npm run flatten && npm run clean-after-flatten",
		"flatten": "copy dist\\resources\\de\\dzhyp\\samplelibrary\\* dist\\",
		"clean": "npx rimraf ExampleLibrary-content.zip dist",
		"clean-after-flatten": "rmdir /s /q dist\\resources dist\\test-resources"
	},
	"devDependencies": {
		"@babel/eslint-parser": "7.14.7",
		"@prettier/plugin-xml": "^3.3.1",
		"@sap-ux/eslint-plugin-fiori-tools": "^0.3.0",
		"@sap/ux-ui5-tooling": "^1.13.0",
		"@sapui5/ts-types-esm": "~1.104.0",
		"@types/openui5": "1.108.0",
		"@ui5/cli": "^3.9.1",
		"@ui5/ts-interface-generator": "^0.7.0",
		"eslint-plugin-fiori-custom": "2.6.7",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.0",
		"prettier-plugin-properties": "^0.2.0",
		"rimraf": "^3.0.2",
		"typescript": "^5.1.6",
		"ui5-middleware-livereload": "^0.8.4",
		"ui5-task-flatten-library": "0.2.5",
		"ui5-tooling-transpile": "^0.7.10"
	}
}
